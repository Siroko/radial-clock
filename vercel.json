{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Cross-Origin-Resource-Policy",
            "value": "cross-origin"
          },
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          },
          {
            "key": "X-Frame-Options",
            "value": "ALLOW-FROM https://felixmartinez.dev"
          },
          {
            "key": "Content-Security-Policy",
            "value": "frame-ancestors 'self' http://localhost:* http://localhost https://felixmartinez.dev https://*.felixmartinez.dev"
          }
        ]
      }
    ]
  }